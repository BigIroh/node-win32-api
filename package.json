{
  "name": "win32-api",
  "version": "1.3.1",
  "description": "FFI definitions of windows win32 api for node-ffi",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {},
  "scripts": {
    "lint": "tslint -e **/*.d.ts -t stylish src/**/*.ts test/**/*.ts",
    "jslint": "eslint --fix dist/**/*.js",
    "test": "npm run test-local",
    "test-local": "mocha --config test/mocha.opts",
    "prepublishOnly": "npm run build",
    "build": "npm run test && rm -rf dist/* && tsc -p ./ --outDir dist/ && npm run jslint",
    "test-ci": "mocha --config test/mocha.travis.opts",
    "build-ci": "npm run test-ci && rm -rf dist/* && tsc -p ./ --outDir dist/ && npm run jslint",
    "test-appveyor": "mocha --config test/mocha.appveyor.opts && rm -rf dist/* && tsc -p ./ --outDir dist/ && npm run jslint"
  },
  "keywords": [
    "windows",
    "win32",
    "api",
    "kernel32",
    "user32",
    "comctl32",
    "ffi"
  ],
  "module": "./dist/index.js",
  "author": "waiting",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waitingsong/node-win32-api.git"
  },
  "bugs": {
    "url": "https://github.com/waitingsong/node-win32-api/issues"
  },
  "homepage": "https://github.com/waitingsong/node-win32-api#readme",
  "engines": {
    "node": ">=6.5.0"
  },
  "os": [
  ],
  "dependencies": {},
  "devDependencies": {
    "@types/ffi": "^0.0.19",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.25",
    "@types/power-assert": "^1.4.29",
    "@types/ref": "^0.0.28",
    "@types/ref-struct": "^0.0.28",
    "eslint": "^4.8.0",
    "ffi": "^2.2.0",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^4.0.1",
    "power-assert": "^1.4.4",
    "rimraf": "^2.6.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0"
  }
}
